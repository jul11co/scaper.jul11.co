      'font-weight: normal;' +
    '}'
    );
    addCss('' +
    '.txt_main_category,' +
    '.txt_main_category a {' +
      'font-weight: bold;' +
      'font-size: 16px;' +
      'color: #444;' +
    '}'
    );

    setHtml($('.main_content_detail').html());
