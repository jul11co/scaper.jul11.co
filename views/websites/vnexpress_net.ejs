<%
if (urlMatch('vnexpress.net')) {

  setScriptFile('vnexpress_net.ejs');

  removeSelector('.block_share_icon');
  removeSelector('.block_view_more');

  removeSelector('.block_timer_share');

  if ($('.main_content_detail').length) {

    removeSelector('.block_input_comment');
    removeSelector('.list_news');
    removeSelector('#box_xemnhieunhat');

    removeSelector('.main_content_detail .clear');
    removeSelector('.main_content_detail .banner_468');

    removeSelector('#right_calculator');
    removeSelector('.social_share');

    $('#box_tinkhac_detail a').each(function(){
      $(this).removeAttr('onclick');      
    });

    removeInlineScripts('.main_content_detail');
    normalizeLinks('.main_content_detail');

    addCss('' +
    '.width_common {' +
      'width: 100%;' +
    '}'
    );
    addCss('' +
    '.block_tag {' +
      'display: inline-block;' +
    '}' +
    '.block_tag .txt_tag {' +
      'background-color: #f1f1f1;' +
      'padding: 0 10px 0 10px;' +
