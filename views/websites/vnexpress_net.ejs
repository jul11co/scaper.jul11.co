    '#news_home.list_news .list_news_dot_3x3 li,' +
    '.list_news .list_news_dot_3x3 li {' +
      'border: none;' +
    '}' +
    'ul.list_news_dot_3x3 {' +
      'list-style: none;' +
      'padding: 0;' +
    '}' +
    '.news_lead a,' +
    '.list_news_dot_3x3 a {' +
      'font-weight: normal;' +
      'color: #999;' +
