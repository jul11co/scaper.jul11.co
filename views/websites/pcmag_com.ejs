<%
if (urlMatch('pcmag.com')) {

  setScriptFile('pcmag_com.ejs');

  removeSelector('.editor_rating');
  removeSelector('.kwnav');
  removeSelector('.score');

  removeSelector('#article_filter');
  removeSelector('#form_filter');

  removeSelector('.thumb.article');

  removeSelector('.breadcrumbs');
  removeSelector('.share_bar');
  removeSelector('.social300');
  removeSelector('.comments');
  removeSelector('#disqus_thread');

  removeSelector('div.sidebar');

  removeSelector('#top-comment-count');

  addCss('' +
  'div.splashImg {' +
    'float: left;' +
    'margin: 5px 10px 0 0;' +
    'max-width: 40%;' +
  '}' +
  '.article #id_text {' +
    'font-size: 16px;' +
    'line-height: 25px;' +
    'letter-spacing: .5px;' +
    'padding-top: 10px;' +
    'position: relative;' +
    '-webkit-font-smoothing: subpixel-antialiased;' +
    'text-rendering: optimizeLegibility;' +
  '}' +
  '.article #id_text p {' +
    'font-size: 16px;' +
    'line-height: 25px;' +
    'letter-spacing: .5px;' +
    'margin-bottom: 15px;' +
  '}' +
  '.article #id_text img.embed {' +
    'max-width: 100%;' +
    'float: left;' +
    'margin: 0 10px 0 0;' +
  '}' +
  '.article .byline {' +
    'font-size: 13px;' +
    'color: #000;' +
    'text-transform: uppercase;' +
    'margin-bottom: 10px;' +
  '}' +
  '.article .byline .sep {' +
    'padding: 0;' +
  '}' +
  'address {' +
    'display: inline;' +
  '}' +
  '#id_deck {' +
    'font-style: italic;' +
    'color: #000;' +
    'margin-top: 10px;' +
    'margin-bottom: 10px;' +
  '}' +
  'ul.taglist {' +
    'list-style: none;' +
    'padding: 0;' +
    'border-top: 1px solid #e9e9e9;' +
    'padding-top: 5px;' +
    'clear: both;' +
    'overflow: hidden;' +
    'margin-top: 15px;' +
  '}' +
  'ul.taglist li {' +
    'float: left;' +
    'margin: 5px;' +
  '}' +
  'ul.taglist a {' +
    'display: inline-block;' +
    'white-space: nowrap;' +
    'padding: 5px 8px 20px 8px;' +
    'background-size: 15px;' +
    'border-radius: 5px;' +
    'font-size: 14px;' +
    'font-weight: bold;' +
    'height: 1em;' +
    'line-height: 1em;' +
    'vertical-align: middle;' +
    'background-color: #e8e7e6;' +
    'color: #333;' +
  '}' +
  '.article-byline ul {' +
    'list-style: none;' +
    'margin: 0;' +
    'padding: 0 0 10px;' +
  '}' +
  '.article-byline ul li {' +
    'display: inline;' +
    'float: left;' +
    'margin-left: 5px;' +
    'text-transform: uppercase;' +
  '}'
  );
  addCss('' +
  'ul.kwfeatured,' +
  'ul.kwlist {' +
    'list-style: none;' +
  '}'
  );

  if ($('#primary-content').length) {

    addCss('' +
    '.article-container .byline,' +
    '#roundup-tag .byline {' +
      'overflow: hidden;' +
      'padding: 5px 0 10px 0;' +
    '}' +
    '.article-container .byline ul,' +
    '#roundup-tag .byline ul,' +
    '.page-nav ul {' +
      'list-style: none;' +
      'margin: 0;' +
      'padding: 0;' +
    '}' +
    '.article-container .byline ul li,' +
    '#roundup-tag .byline ul li {' +
      'color: #000;' +
      'padding: 0 20px 0 0;' +
      'float: left;' +
      'text-transform: uppercase;' +
    '}' +
    '.article-body img.right {' +
      'float: right;' +
      'margin: 0 0 0 10px;' +
    '}'
    );

    removeSelector('.author-container');
    removeSelector('#backtop');
    removeSelector('#login_overlay');
    removeSelector('#loginOverlayDialog');
    removeSelector('div.heading');

    removeSelector('.bottom-navbar');
    removeSelector('.dsq-brlink');

    removeSelector('.related-articles ul > li > a > img');

    removeInlineScripts('#primary-content');
    normalizeLinks('#primary-content');

    setHtml($('#primary-content').html());
  } else {

    removeInlineScripts('div#main');
    normalizeLinks('div#main');

    setHtml($('div#main').html());
  }
}
%>