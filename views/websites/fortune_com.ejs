<%
if (urlMatch('fortune.com')) {

  setScriptFile('fortune_com.ejs');

  removeSelector('.article-byline-twitters');
  removeSelector('.article-social');

  removeInlineScripts('article');
  normalizeLinks('article');

  addCss('' +
  'ul {' +
    'list-style: none;' +
    'margin: 0;' +
    'padding: 0;' +
  '}' +
  'time {' +
    'font-size: 11px;' +
    'font-size: .55rem;' +
    '-webkit-font-smoothing: antialiased;' +
    'color: #828282;' +
    'display: block;' +
    'font-family: "Helvetica Neue Medium Condensed",helvetica,sans-serif;' +
    'font-weight: 100;' +
    'margin-bottom: 0;' +
    'text-transform: uppercase;' +
  '}'
  );

  addCss('' +
  '.article-byline {' +
    'margin-bottom: 18px;' +
    'position: relative;' +
  '}' +
  '.article-byline .article-byline-item {' +
    '-webkit-font-smoothing: antialiased;' +
    'display: inline-block;' +
    'font-family: "Helvetica Neue Medium",helvetica,sans-serif;' +
    'font-size: 15px;' +
    'line-height: 1;' +
    'margin: 0 30px 0 0;' +
    'width: auto;' +
  '}' +
  '.article-byline .article-byline-item li {' +
    'display: inline-block;' +
  '}' +
  '.article-byline .article-byline-item li a {' +
    'font-size: 15px;' +
  '}' +
  '.article-byline .article-byline-item li a span {' +
    'display: inline;' +
    'margin-left: -3px;' +
  '}'
  );

  setHtml($('article').html());
}
%>