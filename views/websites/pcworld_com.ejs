<%
if (urlMatch('pcworld.com')) {

  setScriptFile('pcworld_com.ejs');

  removeSelector('.breadcrumbs');
  removeSelector('.icn-link');
  removeSelector('#primarySocialSharing');

  removeSelector('.gig-comments-headerText');
  removeSelector('#commentsDiv');
  removeSelector('#emailModal');
  removeSelector('.byline.vcard.author');

  removeInlineScripts('div#main');
  normalizeLinks('div#main');

  addCss('' +
  'ul.fmenu {' +
    'list-style: none;' +
    'padding: 0;' +
  '}' +
  '.department {' +
    'margin: 2rem 0;' +
    'overflow: hidden;' +
  '}' +
  '.department ul.tags {' +
    'list-style: none;' +
    'margin: 0;' +
    'padding: 0;' +
  '}' +
  '.department ul.tags li {' +
    'float: left;' +
    'margin-left: 0;' +
  '}' +
  '.department ul.tags li,' +
  '.department ul.tags li a {' +
    'color: #444;' +
    'font-size: 14px;' +
    'line-height: 1;' +
    'text-decoration: none;' +
    'text-transform: capitalize;' +
  '}' +
  '.department ul.tags li {' +
    'margin-top: 6px;' +
    'margin-right: 6px;' +
    'margin-bottom: 6px;' +
    'margin-left: 0;' +
  '}' +
  '.department ul.tags li a {' +
    'background-color: #e8e8e8;' +
    'padding: 10px 10px;' +
    'border-radius: 4px;' +
  '}'
  );

  setHtml($('div#main').html());
}
%>