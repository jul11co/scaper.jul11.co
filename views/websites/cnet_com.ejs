<%
if (urlMatch('cnet.com')) {

  setScriptFile('cnet_com.ejs');

  removeSelector('.breadcrumb');
  removeSelector('.sharebarRedesign');

  removeSelector('div.collapsible');
  removeSelector('.tocfixate');
  removeSelector('.ad-leader-bottom');
  removeSelector('.about-author');
  removeSelector('.fullWidthSixAssets');

  //$('.article-main-body').removeClass('row');

  $('.container').each(function() {
    $(this).removeClass('container');
  });
  $('.row').each(function() {
    $(this).removeClass('row');
  });

  removeInlineScripts('#rbContent');
  normalizeLinks('#rbContent');

  setHtml($('#rbContent').html());
}
%>