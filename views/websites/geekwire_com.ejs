<%
if (urlMatch('geekwire.com')) {

  setScriptFile('geekwire_com.ejs');

  $('img').each(function() {
    $(this).removeAttr('width');
    $(this).removeAttr('height');
  });

  $('figure').each(function() {
    $(this).css('width', 'none');
  });

  removeSelector('#comments');
  removeSelector('.post-sidebar');
  removeSelector('.share');

  removeSelector('#geekwork');

  removeSelector('#subscribe-box');
  removeSelector('#mobile-share-bar');

  removeInlineScripts('.main');
  normalizeLinks('.main');

  $('.entry-content').parent().removeClass('col-md-8').addClass('col-md-12');

  setHtml($('.main').html());
}
%>
