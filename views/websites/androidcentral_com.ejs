<%
if (urlMatch('androidcentral.com') || urlMatch('windowscentral.com')
  || urlMatch('imore.com')) {

  setScriptFile('androidcentral_com.ejs');

  $('img').each(function() {
    $(this).removeAttr('width');
    $(this).removeAttr('height');
  });

  removeSelector('.sub-title');
  removeSelector('#comments');
  removeSelector('.static-social');
  removeSelector('.social');
  removeSelector('div.narrow');
  removeSelector('.posts');
  removeSelector('.articles');
  removeSelector('.article-nav');

  removeSelector('.author-thumb');
  removeSelector('.author-info');

  // windowscentral.com
  removeSelector('.windows-phone-links');
  removeSelector('.meta-comments');
  removeSelector('.social-list');
  removeSelector('.comments-count');
  removeSelector('.stories-count');
  removeSelector('.article-social-actions');
  removeSelector('.spe-share-wrap.entry-unrelated');

  // imore.com
  removeSelector('.wp-about-author-containter-none');

  $('.container').each(function() {
    $(this).removeClass('container');
  });

  removeInlineScripts('.content');
  normalizeLinks('.content');

  setHtml($('.content').html());
}
%>