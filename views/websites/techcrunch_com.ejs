<%
if (urlMatch('techcrunch.com')) {

  setScriptFile('techcrunch_com.ejs');

  removeSelector('.tags');
  removeSelector('.social-share');
  removeSelector('.next-link');
  removeSelector('.ad-unit');

  removeSelector('.l-sidebar');

  removeInlineScripts('.article');
  normalizeLinks('.article');

  addCss('' +
  '.page-title {' +
    'padding-bottom: .5em;' +
    'margin-bottom: 1em;' +
    'position: relative;' +
    'font-size: .95em;' +
  '}'
  );

  setHtml($('.article').html());
}
%>